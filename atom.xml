    <title>python画图中文显示</title>
    <link href="http://yoursite.com/2016/03/28/python%E7%94%BB%E5%9B%BE%E4%B8%AD%E6%96%87%E6%98%BE%E7%A4%BA%E9%97%AE%E9%A2%98/"/>
